prog=$1
quiet=0
prefix='https://ftp.gnu.org/gnu/'
curl --location --output /dev/null --silent --head --fail "$prefix$prog"
type1=$?
curl --location --output /dev/null --silent --head --fail "$prefix$prog.README"
type2=$?
[ $type1 -eq 0 -o $type2 -eq 0 ] && [ $quiet -eq 0 ] && echo "yes" && exit 0
[ $quiet -eq 0 ] && echo "no" && exit 1
